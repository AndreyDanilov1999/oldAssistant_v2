{
    "QWidget": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #000000, stop:1 #0F0D31)",
        "color": "#ffffff",
        "font-size": "13px"
    },
    "QPushButton": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #000000, stop:1 #18154B)",
        "color": "#ffffff",
        "height": "30px",
        "border": "1px solid #4577ff",
        "border-radius": "3px",
        "font-size": "13px"
    },
    "QPushButton:hover": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #18154B, stop:1 #000000)",
        "color": "#ffffff",
        "font-size": "13px"
    },
    "QPushButton:pressed": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #000000, stop:1 #00002d)",
        "padding-left": "3px",
        "padding-top": "3px"
    },
    "TabWidget::pane": {
        "margin": "0px",
        "padding": "0px"
    },
    "QTabBar::tab": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #000000, stop:1 #18154B)",
        "color": "#ffffff",
        "height": "30px",
        "border": "1px solid #4577ff",
        "border-radius": "3px",
        "font-size": "13px",
        "margin": "3px",
        "padding": "3px"
    },
    "QTabBar::tab:selected": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #18154B, stop:1 #000000)",
        "color": "#ffffff",
        "font-size": "13px",
        "margin": "3px",
        "padding": "3px",
        "padding-top": "10px"
    },
    "QLineEdit": {
        "background-color": "transparent",
        "border": "1px solid #4577ff",
        "border-radius": "3px",
        "padding": "5px"
    },
    "QComboBox": {
        "background-color": "transparent",
        "border": "1px solid #4577ff",
        "border-radius": "3px",
        "padding": "5px"
    },
    "QCheckBox": {
        "background-color": "transparent",
        "padding": "2px"
    },
    "QCheckBox::indicator": {
        "width": "12px",
        "height": "12px",
        "border": "1px solid #4577ff",
        "border-radius": "5px",
        "padding": "5px"
    },
    "QCheckBox::indicator:hover": {
        "width": "10px",
        "height": "10px"
    },
    "QCheckBox::indicator:checked": {
        "background-color": "#4577ff"
    },
    "QTextEdit": {
        "background": "transparent",
        "color": "#c2caff",
        "border": "none",
        "font-size": "15px"
    },
    "label_version": {
        "background": "transparent",
        "color": "#c2caff",
        "font-size": "10px"
    },
    "label_message": {
        "color": "#ffffff",
        "font-size": "13px"
    },
    "update_label": {
        "background": "transparent",
        "color": "#c2caff",
        "font-size": "12px"
    },
    "TitleBar": {
        "border-bottom": "1px solid #4577ff"
    },
    "TrayButton": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #000000, stop:1 #18154B)",
        "color": "#ffffff",
        "height": "30px",
        "border": "1px solid #4577ff",
        "border-radius": "3px",
        "font-size": "13px"
    },
    "TrayButton:hover": {
        "color": "#ffffff",
        "background-color": "#0790EC",
        "border": "1px solid #0790EC"
    },
    "CloseButton": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #000000, stop:1 #18154B)",
        "color": "#ffffff",
        "height": "30px",
        "border": "1px solid #4577ff",
        "border-radius": "3px",
        "font-size": "13px"
    },
    "CloseButton:hover": {
        "color": "#ffffff",
        "background-color": "#E04F4F",
        "border": "1px solid #E04F4F"
    },
    "MessageContainer": {
        "border": "1px solid #4577ff",
        "border-radius": "25px"
    },
    "WindowContainer": {
        "border": "1px solid #4577ff",
        "border-radius": "3px"
    },
    "QMenu": {
        "background-color": "qlineargradient(x1:0.85, y1:0.85, x2:0.15, y2:0.15, stop:0 #000000, stop:1 #18154B)",
        "color": "#ffffff",
        "font-size": "13px",
        "padding": "5px"
    },
    "QMenu::item": {
        "padding": "8px 20px",
        "border-radius": "5px"
    },
    "QMenu::item:selected": {
        "border-radius": "5px",
        "background-color": "#4577ff",
        "color": "#ffffff"
    },
    "QColorDialog QPushButton": {
        "padding-left": "5px",
        "padding-right": "5px"
    }
}