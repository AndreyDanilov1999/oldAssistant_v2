{
    "QWidget": {
        "background-color": "#0e111e",
        "color": "#ffffff",
        "font-size": "13px"
    },
    "QPushButton": {
        "background-color": "#191b41",
        "color": "#ffffff",
        "height": "30px",
        "border": "1px solid #4577ff",
        "border-radius": "3px",
        "font-size": "13px"
    },
    "QPushButton:hover": {
        "background-color": "#0f1137",
        "color": "#ffffff",
        "font-size": "13px"
    },
